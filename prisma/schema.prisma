  tps        String?   @db.VarChar(3)
  kec        String?   @db.VarChar(50)
  kel        String?   @db.VarChar(50)
  kodepos    String?   @db.VarChar(5)
  ttl        DateTime? @db.Date
  concat_dpt String?   @db.VarChar(100)
}

